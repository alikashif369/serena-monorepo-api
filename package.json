{
  "name": "serena-monorepo-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:vector-service": "nest build vector-service",
    "build:raster-service": "nest build raster-service",
    "build:auth-service": "nest build auth-service",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "npm run start:dev:vector-service",
    "start:dev:vector-service": "nest start vector-service --watch",
    "start:dev:raster-service": "nest start raster-service --watch",
    "start:dev:auth-service": "nest start auth-service --watch",
    "start:debug": "nest start --debug --watch",
    "start:debug:vector-service": "nest start vector-service --debug --watch",
    "start:debug:raster-service": "nest start raster-service --debug --watch",
    "start:debug:auth-service": "nest start auth-service --debug --watch",
    "start:prod": "concurrently \"npm run start:prod:vector-service\" \"npm run start:prod:raster-service\"",
    "start:prod:vector-service": "node dist/apps/vector-service/main",
    "start:prod:raster-service": "node dist/apps/raster-service/main",
    "start:prod:auth-service": "node dist/apps/auth-service/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/serena-monorepo-api/test/jest-e2e.json",
    "test:vector-service": "jest --config ./apps/vector-service/test/jest-e2e.json",
    "test:raster-service": "jest --config ./apps/raster-service/test/jest-e2e.json",
    "prisma:generate:vector": "npx prisma generate --schema=apps/vector-service/prisma/schema.prisma",
    "prisma:migrate:vector": "npx prisma migrate dev --schema=apps/vector-service/prisma/schema.prisma",
    "prisma:migrate:deploy:vector": "npx prisma migrate deploy --schema=apps/vector-service/prisma/schema.prisma",
    "prisma:studio:vector": "npx prisma studio --schema=apps/vector-service/prisma/schema.prisma",
    "prisma:reset:vector": "npx prisma migrate reset --schema=apps/vector-service/prisma/schema.prisma",
    "prisma:seed:vector": "npx prisma db seed --schema=apps/vector-service/prisma/schema.prisma",
    "prisma:status:vector": "npx prisma migrate status --schema=apps/vector-service/prisma/schema.prisma",
    "prisma:generate:raster": "npx prisma generate --schema=apps/raster-service/prisma/schema.prisma",
    "prisma:migrate:raster": "npx prisma migrate dev --schema=apps/raster-service/prisma/schema.prisma",
    "prisma:migrate:deploy:raster": "npx prisma migrate deploy --schema=apps/raster-service/prisma/schema.prisma",
    "prisma:studio:raster": "npx prisma studio --schema=apps/raster-service/prisma/schema.prisma",
    "prisma:reset:raster": "npx prisma migrate reset --schema=apps/raster-service/prisma/schema.prisma",
    "prisma:seed:raster": "npx prisma db seed --schema=apps/raster-service/prisma/schema.prisma",
    "prisma:status:raster": "npx prisma migrate status --schema=apps/raster-service/prisma/schema.prisma",
    "prisma:generate:auth": "npx prisma generate --schema=apps/auth-service/prisma/schema.prisma",
    "prisma:migrate:auth": "npx prisma migrate dev --schema=apps/auth-service/prisma/schema.prisma",
    "prisma:migrate:deploy:auth": "npx prisma migrate deploy --schema=apps/auth-service/prisma/schema.prisma",
    "prisma:studio:auth": "npx prisma studio --schema=apps/auth-service/prisma/schema.prisma",
    "prisma:reset:auth": "npx prisma migrate reset --schema=apps/auth-service/prisma/schema.prisma",
    "prisma:status:auth": "npx prisma migrate status --schema=apps/auth-service/prisma/schema.prisma",
    "cli:auth": "ts-node -r tsconfig-paths/register apps/auth-service/src/cli.ts",
    "seed:auth": "npm run cli:auth seed:admin",
    "prisma:generate:all": "npm run prisma:generate:vector && npm run prisma:generate:auth && npm run prisma:generate:raster",
    "prisma:migrate:all": "npm run prisma:migrate:vector && npm run prisma:migrate:auth && npm run prisma:migrate:raster",
    "prisma:migrate:deploy:all": "npm run prisma:migrate:deploy:vector && npm run prisma:migrate:deploy:auth && npm run prisma:migrate:deploy:raster",
    "prisma:reset:all": "npm run prisma:reset:vector && npm run prisma:reset:auth && npm run prisma:reset:raster",
    "prisma:status:all": "npm run prisma:status:vector && npm run prisma:status:auth && npm run prisma:status:raster",
    "prisma:generate": "npm run prisma:generate:all",
    "prisma:migrate": "npm run prisma:migrate:all",
    "prisma:migrate:deploy": "npm run prisma:migrate:deploy:all",
    "prisma:studio": "npm run prisma:studio:vector",
    "prisma:reset": "npm run prisma:reset:all",
    "prisma:seed": "npm run prisma:seed:vector && npm run prisma:seed:raster",
    "prisma:status": "npm run prisma:status:all",
    "vector:setup": "npm run prisma:generate:vector && npm run build:vector-service",
    "vector:dev": "npm run prisma:generate:vector && npm run start:dev:vector-service",
    "raster:setup": "npm run prisma:generate:raster && npm run build:raster-service",
    "raster:dev": "npm run prisma:generate:raster && npm run start:dev:raster-service",
    "dev:all": "npm run prisma:generate:all && npm run start:dev",
    "postinstall": "npm run prisma:generate:all"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.9",
    "@nestjs/swagger": "^11.2.0",
    "@prisma/client": "^6.11.1",
    "@types/geojson": "^7946.0.16",
    "@types/minio": "^7.1.0",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "dotenv": "^16.4.5",
    "minio": "^8.0.6",
    "multer": "^2.0.2",
    "nestjs-command": "^3.1.5",
    "nestjs-prisma": "^0.25.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sharp": "^0.34.5",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/multer": "^2.0.0",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.2",
    "concurrently": "^9.1.0",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^30.0.0",
    "prettier": "^3.4.2",
    "prisma": "^6.11.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
